using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.RegularExpressions;

namespace Rextester
{
    public class Program
    {
        public static void Main(string[] args)
        {
            //Your code goes here
            int input01;
            int input02;
            
            Console.Write ("Type a number to be divided: ");
            input01 = Convert.ToInt32 (Console.ReadLine ());
            Console.Write ("Type another number: ");
            input02 = Convert.ToInt32 (Console.ReadLine ());
            Console.Write(input01 + " divided by " + input02 + " equals " + input01/input02);
        }
    }
}
